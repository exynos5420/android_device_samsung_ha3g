# Radio
/dev/link_pm								u:object_r:radio_device:s0
/dev/network_throughput							u:object_r:radio_device:s0

/data/misc/radio(/.*)?							u:object_r:radio_data_file:s0
/sys/devices/platform/s5p-ehci/ehci_power				u:object_r:sysfs_writable:s0

# Daemons
/system/bin/cbd								u:object_r:cpboot-daemon_exec:s0
/system/bin/modemloader							u:object_r:modemloader_exec:s0
/system/bin/macloader							u:object_r:macloader_exec:s0
/system/bin/sensorhubservice						u:object_r:sensorhubservice_exec:s0

# CP device
/dev/spi_boot_link							u:object_r:radio_device:s0

# Sensors
/efs/hw_offset								u:object_r:sensors_efs_file:s0
/efs/gyro_cal_data							u:object_r:sensors_efs_file:s0
/efs/calibration_data							u:object_r:sensors_efs_file:s0
/efs/FactoryApp/baro_delta						u:object_r:sensors_efs_file:s0
/dev/ssp_sensorhub							u:object_r:sensorhub_dev:s0
/dev/batch_io								u:object_r:batch_io_device:s0
/dev/shtc1_sensor							u:object_r:sensorhub_dev:s0

