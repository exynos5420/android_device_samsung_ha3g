# CS service
service cs_service /system/bin/cs
    class main
    user system
    group system
    disabled

on property:sys.mobicoredaemon.enable=true
    start cs_service

# SSWAP
service sswap /sbin/sswap -s
    class core
    user root
    group root
    oneshot
    seclabel u:r:sswap:s0
